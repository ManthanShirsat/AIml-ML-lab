    "data = load_breast_cancer()\n",
    "\n",
    "x=data.data\n",
    "y=data.target\n",
    "\n",
    "print(\"Classes:\",data.target_names)\n",
    "\n",
    "X_train,X_test,Y_train,Y_test = train_test_split(x,y,test_size=0.3)"
   ]
